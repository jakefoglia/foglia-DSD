			<point x="60" y="-10"/>
		</path>
	</wire>
	<component id="0" def="And" x="-20" y="-50" direction="right" flip="false">
		<connection flow="in" pinid="0">
			<subpin wireid="1" invert="false"/>
			<subpin wireid="2" invert="false"/>
		</connection>
		<connection flow="out" pinid="1" wireid="6" invert="false"/>
