			<point x="-100" y="-40"/>
			<point x="-55" y="-40"/>
		</path>
		<marker x="-100" y="-100"/>
		<tag type="interface" xwire="-230" ywire="-100" xtag="-235" ytag="-100" flow="in" pinid="2" text="C In"/>
	</wire>
	<wire id="3">
		<path>
			<point x="-190" y="-120"/>
			<point x="-190" y="0"/>
			<point x="-55" y="0"/>
		</path>
		<path>
			<point x="-230" y="-120"/>
			<point x="-175" y="-120"/>
		</path>
		<marker x="-190" y="-120"/>
