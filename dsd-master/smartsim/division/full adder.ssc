	</wire>
	<wire id="4">
		<path>
			<point x="-210" y="-140"/>
			<point x="-210" y="20"/>
			<point x="-55" y="20"/>
		</path>
		<path>
			<point x="-230" y="-140"/>
			<point x="-175" y="-140"/>
		</path>
		<marker x="-210" y="-140"/>
		<tag type="interface" xwire="-230" ywire="-140" xtag="-235" ytag="-140" flow="in" pinid="0" text="A"/>
	</wire>
	<wire id="5">
		<path>
